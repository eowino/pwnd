{
  "name": "pwnd",
  "version": "1.0.0",
  "description": "A CLI that checks if your passwords have been compromised in a data breach",
  "main": "./bin/index.js",
  "bin": {
    "pwnd": "./bin/index.js"
  },
  "scripts": {
    "clean": "rm -rf ./bin",
    "prebuild": "npm run clean",
    "build": "tsc",
    "start": "tsc -w",
    "tslint": "tslint -c tslint.json -p tsconfig.json --fix",
    "prepublish": "npm run tslint && npm run build",
    "precommit": "./scripts/pre-commit-check.sh && pretty-quick --staged && npm run tslint",
    "format": "prettier --write src/**/*.{js,md,ts,json}"
  },
  "files": [
    "lib/**/*",
    "bin/**/*"
  ],
  "engines": {
    "node": ">=6.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run precommit"
    }
  },
  "keywords": [],
  "author": "Evans Owino",
  "homepage": "https://github.com/eowino/pwnd",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.18.0",
    "sha1": "^1.1.1"
  },
  "devDependencies": {
    "@types/node": "^11.11.2",
    "@types/sha1": "^1.1.1",
    "@types/signale": "^1.2.1",
    "husky": "^1.3.1",
    "prettier": "^1.16.4",
    "pretty-quick": "^1.10.0",
    "signale": "^1.4.0",
    "tslint": "^5.13.1",
    "typescript": "^3.3.3333"
  }
}
